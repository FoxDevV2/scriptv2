-- Scripts:

local function DNGV_fake_script() -- FazCoins.LocalScript 
	local script = Instance.new('LocalScript', FazCoins)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.FazCoinsFrame.Visible = true
	end)
end
coroutine.wrap(DNGV_fake_script)()
local function TQBIA_fake_script() -- Tickets.LocalScript 
	local script = Instance.new('LocalScript', Tickets)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.TicketsFrame.Visible = true
	end)
end
coroutine.wrap(TQBIA_fake_script)()
local function LNBF_fake_script() -- Return.LocalScript 
	local script = Instance.new('LocalScript', Return)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(LNBF_fake_script)()
local function BAON_fake_script() -- Return_2.LocalScript 
	local script = Instance.new('LocalScript', Return_2)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(BAON_fake_script)()
local function BQSC_fake_script() -- Close.LocalScript 
	local script = Instance.new('LocalScript', Close)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(BQSC_fake_script)()
